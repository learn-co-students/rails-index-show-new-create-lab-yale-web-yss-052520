<h1>Coupon List</h1>

<table>
    <tr>
        <th>Coupon Code</th>
        <th>Store</th>
    </tr>
    <%= @coupons.each do |coupon| %>
        <tr>
            <td><%= link_to(coupon.coupon_code, coupon_path(coupon)) %></td>
            <td><%= coupon.store %></td>
        </tr>
    <% end %>
</table>